{
  "name": "nome-do-seu-projeto",
  "version": "1.0.0",
  "description": "Exemplo de projeto Node.js com ES Modules",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "prepare": "husky",
    "knex": "node --no-warnings --import tsx ./node_modules/knex/bin/cli.js",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "eslint .",
    "lint": "eslint src --ext .ts --fix",
    "commit": "cz",
    "test": "vitest",
    "build": "tsup src --out-dir build"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "fastify": "^4.12.0",
    "knex": "^2.5.1",
    "pg": "^8.9.0",
    "zod": "^3.20.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caioolopes/Node.js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caioolopes/Node.js/issues"
  },
  "homepage": "https://github.com/caioolopes/Node.js#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@fastify/cookie": "^11.0.2",
    "@types/supertest": "^6.0.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "fastify": "^5.6.1",
    "husky": "^9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}